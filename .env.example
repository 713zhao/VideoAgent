## Example environment variables for VideoAgent
# Copy this file to `.env` and fill in your secrets. Do NOT commit real secrets.

# Email (SMTP) settings - either set `EMAIL_PASSWORD` or update config.yaml with password
EMAIL_PASSWORD=

# OpenAI-compatible API key (if using openai_compatible backend)
OPENAI_API_KEY=

# Gemini API key (if using gemini backend). Can be the actual key or an env var name.
GEMINI_API_KEY=

# Telegram bot credentials (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Optional: other secrets used by custom integrations
TWITTER_BEARER_TOKEN=
